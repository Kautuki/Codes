        {
            return -1;
        }
        int[] temp=new int[n];
        int j=0;
        int ans=-1;
        for(int i=target;i<n;i++)
        {
            temp[j]=nums[i];
            if(temp[j]==target)
            {
                ans=j;
            }
            j++;
            
        }
        
